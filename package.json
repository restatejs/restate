{
  "name": "@restatejs/restate",
  "version": "1.0.0-alpha.4",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./types/index.d.ts"
    },
    "./implementations/httpClient/axios.js": {
      "import": "./dist/implementations/httpClient/axios.js",
      "types": "./types/implementations/httpClient/axios.d.ts"
    },
    "./implementations/store/vue.js": {
      "import": "./dist/implementations/store/vue.js",
      "types": "./types/implementations/store/vue.d.ts"
    },
    "./models/CoreModel.js": {
      "import": "./dist/models/CoreModel.js",
      "types": "./types/models/CoreModel.d.ts"
    },
    "./models/BaseModel.js": {
      "import": "./dist/models/BaseModel.js",
      "types": "./types/models/BaseModel.d.ts"
    },
    "./plugins/vue.js": {
      "import": "./dist/plugins/vue.js",
      "types": "./types/plugins/vue.d.ts"
    }
  },
  "author": "Everton Andrade <everton@andrade.mat.br>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/restatejs/restate"
  },
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && webpack",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "lint": "eslint . --ext=.ts --fix",
    "test": "jest",
    "publish": "npm publish --access public"
  },
  "peerDependencies": {
    "axios": "0.26.1",
    "vue": "^3.2.31"
